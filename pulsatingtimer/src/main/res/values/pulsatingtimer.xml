<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PulsatingTimer">
        <attr name="android:textColor" format="color"/>
        <attr name="android:backgroundTint" format="color"/>
        <attr name="android:max" format="integer"/>
        <attr name="android:progress" format="integer"/>
        <attr name="android:textSize" format="dimension"/>
        <attr name="android:fontFamily" format="reference"/>
        <attr name="circleRadius" format="dimension"/>
        <attr name="pulsationAlpha" format="fraction"/>
        <attr name="pulsationIntervalMillis" format="integer"/>
        <attr name="pulsationDuration" format="integer"/>
        <attr name="pulsationColor" format="color"/>
    </declare-styleable>
</resources>
